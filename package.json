{
    "name": "podcast-maker",
    "version": "1.0.0",
    "main": "dist/server.js",
    "license": "MIT",
    "scripts": {
        "video:preview": "remotion preview video/src/index.tsx --props='{\"filename\": \"example\", \"destination\": \"youtube\"}'",
        "video:remotion-debug": "remotion render video/src/index.tsx Main out.mp4 --props='{\"filename\": \"example\"}'",
        "video:make:tts": "ts-node src/main.ts --build --config tts",
        "video:make:youtube": "ts-node src/main.ts --build --config youtubeCreate ",
        "video:make:instagram": "ts-node src/main.ts --build --config instagramCreate ",
        "video:make:podcast": "ts-node src/main.ts --build --config podcastCreate ",
        "video:upload:youtube": "ts-node src/main.ts --build --config youtubeUpload",
        "video:upload:instagram": "ts-node src/main.ts --build --config instagramUpload",
        "video:upload:anchor": "ts-node src/main.ts --build --config anchorUpload",
        "video:full:youtube": "ts-node src/main.ts --build --config youtube --tts",
        "video:full:instagram": "ts-node src/main.ts --build --config instagram --tts",
        "video:full:anchor": "ts-node src/main.ts --build --config anchor --tts",
        "tmp:clean": "ts-node src/main.ts --clean",
        "content:create": "ts-node src/main.ts --create",
        "content:mail": "ts-node src/main.ts --mailToContent",
        "content:validate": "ts-node src/main.ts --validate",
        "remotion:upgrade": "remotion upgrade"
    },
    "pre-commit": [
        "content:validate"
    ],
    "dependencies": {
        "@remotion/bundler": "^2.0.2",
        "@remotion/cli": "^2.0.2",
        "@remotion/eslint-config": "^2.0.2",
        "@remotion/media-utils": "^2.0.4",
        "@remotion/renderer": "^2.6.1",
        "@types/node": "^14.6.1",
        "audioconcat": "^0.1.4",
        "axios": "^0.21.1",
        "commander": "^7.2.0",
        "date-fns": "^2.21.1",
        "dotenv": "^8.2.0",
        "google-auth-library": "^7.0.3",
        "googleapis": "^68.0.0",
        "googleapis-common": "^5.0.5",
        "http": "^0.0.1-security",
        "microsoft-cognitiveservices-speech-sdk": "^1.15.1",
        "mp3-duration": "^1.1.0",
        "node-html-parser": "^4.1.4",
        "pre-commit": "^1.2.2",
        "puppeteer": "^8.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.2.0",
        "remotion": "^2.0.2",
        "rss-parser": "^3.12.0",
        "string-width": "^4.2.2",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.8"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/node": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-decorators": "^7.12.12",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@types/react": "^17.0.3",
        "@types/styled-components": "^5.1.9",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
        "@typescript-eslint/parser": "^3.9.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-transform-typescript-metadata": "^0.3.1",
        "eslint": "^7.15.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-typescript": "^2.2.1",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3"
    }
}
